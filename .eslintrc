{
  "extends": [
    "@antfu",
    "@unocss"
  ],
  "ignorePatterns": [
    "**/*.css",
    "**/*.scss",
    "**/*.vsix",
    "**/*.sh",
    "**/*.svg",
    "**/*.yml",
    "**/*.html",
    "**/*.json"
  ],
  "rules": {
    "no-console": "off",
    "prefer-const": 1,
    "max-len": ["error", { "code": 90, "ignoreComments": true, "ignoreUrls": true, "ignorePattern": "^[(\\s|')].*[('|';)]$" }],
    "no-void": "off"
  }
}
